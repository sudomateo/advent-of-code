package {{ .Day }}

import (
	"bufio"
	"fmt"
	"io"
)

func Part1(r io.Reader) (int, error) {
	scanner := bufio.NewScanner(r)
	for scanner.Scan() {
		line := scanner.Text()
		fmt.Println(line)
	}

	return 0, nil
}

