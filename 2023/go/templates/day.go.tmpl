import (
	"bufio"
	"fmt"
	"io"
)

// Part01 implements the solution for part 1.
func Part01(r io.Reader) (string, error) {
	scanner := bufio.NewScanner(r)
	for scanner.Scan() {
		line := scanner.Text()
		fmt.Println(line)
	}

	return "", nil
}

